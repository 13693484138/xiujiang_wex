<!-- 申请分销01 -->
<view class='content1'>
  <view class='content1-text'>姓名\n</view>
  <view class='content11'>
    <view class='content11-1'></view>
    <input id='name' class='content11-input' placeholder='请输入您的姓名' 
    placeholder-style='font-size: 26rpx;color: #888888;letter-spacing: 1px;'
    bindinput='dataInput'></input>
    <view class='content11-2'></view>
  </view>
  <view class='error hidden'>请输入正确的姓名！</view>
</view>


