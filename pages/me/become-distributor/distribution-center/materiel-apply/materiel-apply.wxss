.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-konggouwuche:before { content: "\e645"; font-size: 70px; color: #888888;}

.no-date {
  position: fixed;
  left: 50%;
  top: 30%;
  transform: translate(-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.no-date view:nth-child(2) {
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #888888;
}
