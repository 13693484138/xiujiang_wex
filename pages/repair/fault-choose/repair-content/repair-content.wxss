.alert-radio {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: #f2f2f2;
  height: 96rpx;
  padding-left: 50rpx;
  padding-right: 29rpx;
}
.alert-radio view:nth-child(1) {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.alert-radio-text {
  font-size: 28rpx;
  color: #34322e;
}



/* 弹出窗 */
.alert-content {
  width: 100%;
  height: 244rpx;
  background-color:#f9f9f9;
  position: fixed;
  bottom: 0rpx;
  z-index: 9999;
  font-size: 27rpx;
  color: #010101;
  padding-top: 106rpx;
}
.alert-content > view:nth-child(1) {
  margin-bottom: 90rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  text-align: center;
}
.alert-content > view:nth-child(1) view {
  width: 90rpx;
  height: 40rpx;
  flex: 1;
}
.alert-content > view:nth-child(2) {
  display: flex;
  flex-direction: row;
  justify-content: center;
  text-align: center;
}
.alert-content > view:nth-child(2) view {
  width: 90rpx;
  height: 40rpx;
  flex: 1;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}


.select {
  display: inline-block;
  width: 95rpx;
  height: 40rpx;
  background-color: #000000;
  color: #ffffff;
  border-radius: 10rpx;
}