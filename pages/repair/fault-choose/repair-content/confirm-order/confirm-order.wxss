.co-top {
  width: 100%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 222rpx;
  font-size: 28rpx;
  color: #757575;
  margin-top: 25rpx;
  position: relative;
}
.iconfont {
  margin-bottom: 30rpx;
}
.co-top-black {
  height: 9rpx;
  width: 15rpx;
  background-color: #2e2e2e;
  flex: 1;
  margin-right: 24rpx;
}
.co-top-black:last-child {
  margin-right: 0rpx;
}
.co-top > view:nth-child(2) {
  width: 100%;
  display: flex;
  flex-direction: row;
  position: absolute;
  bottom: 0rpx;
}
.co-top-1 {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.co-top-2 {
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.co-top-2 > view:nth-child(1) {
  font-size: 28rpx;
  color: #2b2b2b;
}
.co-top-2 > view:nth-child(2) {
  margin-top: 25rpx;
  font-size: 26rpx;
  color: #787878;
}
.co-top-2 > view:nth-child(3) {
  margin-top: 18rpx;
  font-size: 20rpx;
  color: #9a9a9a;
  text-decoration: underline;
  float: right;
}



