.alert-content {
  width: 100%;
  height: 375rpx;
  background-color:#f9f9f9;
  position: fixed;
  bottom: 0rpx;
  z-index: 9999;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.radios {
