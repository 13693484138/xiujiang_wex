<view class='list'>
  <view class='list1' wx:for="{{addressList}}" wx:for-item="item" data-addressId="{{item.id}}" bindtap='enterAddressInfo'>
    <view>
      {{item.name}}，{{item.phone}}
    </view>
    <text space='ensp'>{{item.province}} {{item.city}} {{item.district}} {{item.address}}</text>
    <view class='icon1'>
      <view class='iconfont icon-trashbin'></view>
    </view>
  </view>
</view>



<view class='btn1'>
  <view class='btn2' bindtap='goToAddAddress'>新增地址</view>
</view>
