page{
  background: rgb(241, 236, 236);
  height: 100%;
}
.content{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.section-left{
  width: 200rpx;
  height: 100%;
  padding-top: 8rpx;
  background: rgb(236, 247, 250);
}
.section-left-item{
  width: 199rpx;
  height: 117rpx;
  background: #FEAA42;
  margin-top: 4rpx;
  border-radius: 15%;
  color:#FFFFFF;
  vertical-align: middle;
  text-align: center;
  line-height: 117rpx;
  font-size: 30rpx;
  overflow: hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
}
.section-left-item text{
  align-self: center;
  overflow: hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
  font-size: 30rpx;
}
.section-right{
  height: 100%;
  width: 1;
  flex: 1;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.section-right-title{
  text-align: center;
  margin-top: 80rpx;
  margin-bottom: 80rpx;
  text-decoration:underline #AAAAAA;
  color: #AAAAAA;
  font-family: "微软雅黑";
}
.section-right-item-view{
  width: 156rpx;
  border: solid 1rpx #FFFFFF;
  border-radius: 15%;
  padding-top: 10rpx;
  display: inline-block;
  margin-right: 20rpx;
  box-sizing: border-box;
  text-align: center;
  margin-bottom: 60rpx;
}
.section-left-item.active {
  background: #FFFFFF;
  color: #FEAA42;
}

.section-right-item-view:nth-child(3n){
  margin-right: 0rpx;
}
.section-right-item-view view{
  overflow: hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
  font-size: 25rpx;
  color: #AAAAAA;
}
.version-img{
  height: 120rpx;
  width: 100rpx;
}